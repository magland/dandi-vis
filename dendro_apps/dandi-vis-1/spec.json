                {
                    "name": "input",
                    "description": "Input NWB file"
                }
            ],
            "inputFolders": [],
            "outputs": [
                {
                    "name": "output",
                    "description": "Output .nh5 file"
                }
            ],
            "outputFolders": [],
            "parameters": [
                {
                    "name": "units_path",
                    "description": "Path to units within NWB file, default: '/units'",
                    "type": "str",
                    "default": "/units"
